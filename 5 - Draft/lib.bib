	journal = {Epidemiology (Cambridge, Mass.)},
	author = {Sugimoto, Jonathan D. and Borse, Nagesh N. and Ta, Myduc L. and Stockman, Lauren J. and Fischer, Gayle E. and Yang, Yang and Halloran, M. Elizabeth and Longini, Ira M. and Duchin, Jeffrey S.},
