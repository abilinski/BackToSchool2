	keywords = {COVID-19, epidemics, disease transmission, interventions},
	file = {Full Text PDF:/Users/abilinski/Zotero/storage/KRBFR5PC/Tupper et al. - 2020 - Event-specific interventions to minimize COVID-19 .pdf:application/pdf;Snapshot:/Users/abilinski/Zotero/storage/32YIJ9DF/2019324117.html:text/html}
