	author = {Fung, Hannah F and Martinez, Leonardo and Alarid-Escudero, Fernando and Salomon, Joshua A and Studdert, David M and Andrews, Jason R and Goldhaber-Fiebert, Jeremy D and {Stanford-CIDE Coronavirus Simulation Model (SC-COSMO) Modeling Group} and Chin, Elizabeth T and Claypool, Anneke L and Fernandez, Mariana and Gracia, Valeria and Luviano, Andrea and Rosales, Regina Isabel Medina and Reitsma, Marissa and Ryckman, Theresa},
