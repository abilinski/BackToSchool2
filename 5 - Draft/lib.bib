	journal = {JAMA Pediatrics},
	author = {Han, Mi Seon and Choi, Eun Hwa and Chang, Sung Hee and Jin, Byoung-Lo and Lee, Eun Joo and Kim, Baek Nam and Kim, Min Kyoung and Doo, Kihyun and Seo, Ju-Hee and Kim, Yae-Jean and Kim, Yeo Jin and Park, Ji Young and Suh, Sun Bok and Lee, Hyunju and Cho, Eun Young and Kim, Dong Hyun and Kim, Jong Min and Kim, Hye Young and Park, Su Eun and Lee, Joon Kee and Jo, Dae Sun and Cho, Seung-Man and Choi, Jae Hong and Jo, Kyo Jin and Choe, Young June and Kim, Ki Hwan and Kim, Jong-Hyun},
